<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rr"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:layout_marginLeft="8dp"
    android:background="?android:attr/selectableItemBackgroundBorderless"
    app:cardCornerRadius="20dp"
    app:cardElevation="25dp">

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?android:attr/selectableItemBackgroundBorderless">

        <view
            android:id="@+id/view5"
            class="android.support.design.widget.AppBarLayout"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_above="@+id/r"
            android:layout_alignParentStart="true"

            android:layout_marginBottom="-215dp"
            android:background="#0a0b0a" />

        <view
            android:id="@+id/view3"
            class="android.support.design.widget.AppBarLayout"

            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="#e61212" />

        <view
            android:id="@+id/view2"
            class="android.support.design.widget.AppBarLayout"


            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentStart="true"

            android:layout_alignTop="@+id/img2"
            android:background="#0a0b0a" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img"
            android:layout_width="92dp"
            android:layout_height="65dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_below="@+id/textViewtitle"
            android:layout_marginTop="17dp"
            android:scaleType="centerCrop"
            android:src="@drawable/blender"
            app:civ_border_color="#080808"
            app:civ_border_width="2dp" />

        <TextView
            android:id="@+id/textViewname"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="102dp"
            android:layout_marginTop="27dp"
            android:text="TextView"
            android:textColor="@android:color/holo_blue_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="sans" />

        <TextView
            android:id="@+id/textViewdate"
            android:layout_width="62dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textViewname"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="63dp"
            android:fontFamily="sans-serif-condensed"
            android:text="TextView"
            android:textColor="@android:color/holo_blue_bright"
            android:textSize="12sp"
            android:typeface="sans" />

        <TextView
            android:id="@+id/textViewtitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/view5"
            android:layout_below="@+id/view4"
            android:autoLink="web"
            android:fontFamily="sans-serif"
            android:linksClickable="false"

            android:text="TextView"
            android:textColor="@android:color/holo_purple"
            android:textColorLink="#e6128a"
            android:textSize="14sp"
            android:textStyle="bold"
            android:typeface="serif" />

        <view
            android:id="@+id/view"
            class="android.support.design.widget.AppBarLayout"

            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:background="#e6128a" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img2"
            android:layout_width="72dp"
            android:layout_height="46dp"
            android:layout_below="@+id/r"
            android:layout_marginLeft="30dp"
            android:src="@drawable/ic_unlike" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img4"
            android:layout_width="72dp"
            android:layout_height="46dp"

            android:layout_alignTop="@+id/img3"
            android:layout_marginEnd="-102dp"
            android:layout_toStartOf="@+id/textViewname"
            android:src="@drawable/ic_like"
            android:visibility="gone" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img3"
            android:layout_width="72dp"
            android:layout_height="46dp"
            android:layout_alignParentEnd="true"

            android:layout_alignTop="@+id/img2"
            android:layout_marginEnd="115dp"
            android:src="@drawable/ic_comment" />


        <view
            android:id="@+id/view4"
            class="android.support.design.widget.AppBarLayout"

            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="89dp"
            android:background="#0b0b0b" />

        <TextView
            android:id="@+id/textViewpostid"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="127dp"
            android:layout_marginEnd="36dp"
            android:text="TextView"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textViewlikes"
            android:layout_width="wrap_content"
            android:layout_height="33dp"
            android:layout_alignBottom="@+id/img2"
            android:layout_alignParentStart="true"
            android:layout_marginStart="112dp"
            android:text="TextView"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textcomments"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignBottom="@+id/img2"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="47dp"
            android:text="TextView" />

        <RelativeLayout
            android:id="@+id/r"
            android:layout_width="match_parent"
            android:layout_height="281dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="218dp"
            android:background="?android:attr/selectableItemBackgroundBorderless"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textViewquestion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="1dp"
                android:autoLink="web"
                android:fontFamily="serif"
                android:linksClickable="false"
                android:text="TextView"
                android:textColor="@android:color/black"
                android:textColorLink="#e6128a"
                android:textSize="18sp"
                android:typeface="serif" />

            <com.example.khanbros.blender4umodel.squareimageview
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/blender" />

            <com.example.khanbros.blender4umodel.squarevideoview
                android:id="@+id/videoView2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"

                android:layout_marginTop="1dp" />

            <com.google.android.youtube.player.YouTubeThumbnailView
                android:id="@+id/y"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop" />

            <ImageView
                android:id="@+id/imageViewyb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                app:srcCompat="@drawable/yb" />

            <ProgressBar
                android:id="@+id/progressBar2"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />

            <ImageView
                android:id="@+id/imageViewplay"
                android:layout_width="80dp"
                android:layout_height="54dp"
                android:layout_centerInParent="true"
                android:visibility="gone"
                app:srcCompat="@drawable/ic_play" />

        </RelativeLayout>


    </RelativeLayout>


</android.support.v7.widget.CardView>